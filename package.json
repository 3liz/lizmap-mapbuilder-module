  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "eslint-plugin-jsdoc": "^50.6.2",
    "stylelint": "^16.13.2",
    "stylelint-config-standard": "^37.0.0"
  }
}
